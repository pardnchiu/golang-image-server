{
    "apps": [
        {
            "interpreter": "./node_modules/.bin/ts-node",
            "interpreter_args": "-r tsconfig-paths/register",
            "name": "app",
            "cwd": "./",
            "script": "app.ts",
            "exec_mode": "fork",
            "time": true,
            "watch": false,
            "ignore_watch": [
                "./.git",
                "./public",
                "./storage"
            ],
            "out_file": "storage/logs/out.log",
            "error_file": "storage/logs/err.log",
            "combine_logs": true,
            "merge_logs": true,
            "env": {}
        }
    ]
}